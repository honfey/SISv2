@model IEnumerable<SISv2.Models.Course_Module>

<h2>Assign Module to Course</h2>

<p>
    @Html.ActionLink("Assign Module to Course", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Course.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Module.Name)
        </th>
     

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        if (item.st == 1)
        { 
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Course.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Module.Name)
            </td>
          

            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }
  }
</table>
