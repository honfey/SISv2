@model IEnumerable<SISv2.Models.Module>

@{
    ViewBag.Title = "Module";
}

<h2>Module</h2>

<p>
    @Html.ActionLink("Create a New Module", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cb)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ud)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ub)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.st)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cb)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ud)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ub)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.st)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ModuleCode }) |
            @*@Html.ActionLink("Details", "Details", new { id=item.ModuleCode }) |*@
            @Html.ActionLink("Delete", "Delete", new { id=item.ModuleCode })
        </td>
    </tr>
}

</table>
