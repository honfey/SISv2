@model IEnumerable<SISv2.Models.Course>

@{
    ViewBag.Title = "Index";
}

<h2>Course</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
     
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
     
        <th></th>
    </tr>

@foreach (var item in Model)
{
    if (item.st == 1)
    {

    <tr>
        

        <td>
            @Html.ActionLink(item.Name, "Edit", new { id = item.CourseCode })
        </td>

        <td>
            @Html.ActionLink("Delete", "Delete", new { id = item.CourseCode })
        </td>
    </tr>
    }
}
</table>
