@model IEnumerable<SISv2.Models.Intake>

@{
    ViewBag.Title = "Intake";
}

<br />

<p>
    @Html.ActionLink("Create New Intake", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Course.Name)
        </th>
        <th>
            Month
        </th>
        <th>
            Year
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        if (item.st == 1)
        {
        <tr>
            <td>
                @Html.ActionLink(item.Course.Name, "Edit", new { id = item.Id })
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Month.Month1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Year.Year1)
            </td>
            <td>
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
        }
    }
</table>
